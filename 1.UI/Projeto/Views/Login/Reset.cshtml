@{
    Layout = null;
    ViewData["Title"] = "Recuperação senha";
}
<link href="~/css/login.css" rel="stylesheet"/>
<head>
    <title>Recuperar Senha</title>
</head>
<section>
    <form id="forgotForm-146">
        <h1>Digite a nova SENHA!</h1>
        <img id="logoCadeado" src="~/images/logocadeadologin.png"  alt="Logo Cadeado">
        <input type="password"    id="senha-146"            name="nova senha" placeholder="Digite a senha"/>
        <input type="password"    id="confirpassword-146"   name="nova senha" placeholder="Digite novamente a senha"/>
        <button type="submit" id="btnForgot">Criar nova senha</button>
    </form> 
</section>


<!-- Esse código importa o jquery.js "-->
<script src="~/lib/jquery/dist/jquery.js"></script>

<!-- Esse código importa o Login.js na sua página de login. Ele diz ao navegador: 
"Pegue o arquivo Login.js que está na pasta wwwroot/js/ e execute-o nesta página."-->
<script src="~/js/reset.js"></script>
